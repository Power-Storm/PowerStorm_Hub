local currentGame = tostring(game.PlaceId)
if currentGame == "537413528" then -- Build A Boat For Treasure
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Power-Storm/PowerStorm_Hub/main/Database/Games/BABFT"))()
elseif currentGame == "142823291" then -- Murder Mystery 2
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Power-Storm/PowerStorm_Hub/main/Database/Games/MM2"))()
end
