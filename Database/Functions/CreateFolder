local localPlayer = game.Players.LocalPlayer

if isfolder("PowerStorm_Database") then
    if isfolder("PowerStorm_Database/"..localPlayer.UserId) then
    else
        makefolder("PowerStorm_Database/"..localPlayer.UserId)
    end
else
    makefolder("PowerStorm_Database")
    makefolder("PowerStorm_Database/"..localPlayer.UserId)
end
