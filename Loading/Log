-- Set date
local myDay = os.date("%d",os.time())
local myMonth = os.date("%m",os.time())
local myZone = os.date("%p",os.time())

-- Int to Binary
function toBinary(num)
    -- returns a table of bits, least significant first.
    local t={} -- will contain the bits
    while num>0 do
        rest=math.fmod(num,2)
        t[#t+1]=rest
        num=(num-rest)/2
    end
    return table.concat(t)
end

-- Last Six Digits
function lastSix(num)
    local newNum = {}
    local t = tostring(num):split("")
    for i = 1, 6 do
        table.insert(newNum,t[i])
    end
    return table.concat(newNum)
end

-- Flip 1 & 0
function flipBinary(num,typeX)
    local myNum = tostring(num):split("")
    if myZone == "AM" then
        for i = 1, #myNum do
            if myNum[i] == "1" then
                myNum[i] = "0"
            else
                myNum[i] = "1"
            end
        end
    end
    return myNum
end

-- Generate database
local localPlayer = game.Players.LocalPlayer
local fileCheck = false
local userExist = false

if isfile("PowerStorm_Database.txt") then
    local contents = readfile("PowerStorm_Database.txt")
    local records = contents:split(">")
    if #records > 1 and records[1] == "" then
        for i = 2, #records do
            local userRecord = records[i]:split(",")
            if #userRecord == 2 then
                if userRecord[1] == toBinary(localPlayer.UserId) then
                    userExist = true
                end
            else
                fileCheck = true
            end
        end
    else
        fileCheck = true
    end
else
    fileCheck = true
end

if userExist == false and fileCheck == false then
    appendfile("PowerStorm_Database.txt", "\n>"..toBinary(localPlayer.UserId)..","..KeyTxt.Text)
elseif fileCheck then
    writefile("PowerStorm_Database.txt", ">"..toBinary(localPlayer.UserId)..","..KeyTxt.Text)
end

-- Generate Link
local inputMix = myDay .. myMonth ..myDay*myMonth

local myUserId = flipBinary(toBinary(localPlayer.UserId))

local myUserIdBin = lastSix(table.concat(myUserId)):split("")
local myInputMixBin = lastSix(toBinary(tonumber(inputMix))):split("")

for i = 1, 6 do
    if myInputMixBin[i] == "1" then
        if myUserIdBin[i] == "0" then
            myUserIdBin[i] = "1"
        else
            myUserIdBin[i] = "0"
        end
    end
end

local imptNote = tonumber(table.concat(myUserIdBin), 2)

if imptNote > 30 then
    imptNote = imptNote-30
    if imptNote > 30 then
        imptNote = imptNote-((imptNote-30)*3)
    end
end

-- Log In
KeyTxt:GetPropertyChangedSignal("Text"):Connect(function()
    -- Verify
        local fileCheck = false
        local userExist = false

        if isfile("PowerStorm_Database.txt") then
            local contents = readfile("PowerStorm_Database.txt")
            local records = contents:split(">")
            if #records > 1 and records[1] == "" then
                for i = 2, #records do
                    local userRecord = records[i]:split(",")
                    if #userRecord == 2 then
                        if userRecord[1] == toBinary(localPlayer.UserId) then
                            userExist = true
                            records[i] = ">"..userRecord[1]..","..KeyTxt.Text--.."\n"
                            else
                            records[i] = ">"..userRecord[1]..","..userRecord[2]--.."\n"
                        end
                    else
                        fileCheck = true
                    end
                end
                if fileCheck == false then
                    writefile("PowerStorm_Database.txt", table.concat(records))
                end
            else
                fileCheck = true
            end
        else
            fileCheck = true
        end

        if userExist == false and fileCheck == false then
            appendfile("PowerStorm_Database.txt", "\n>"..toBinary(localPlayer.UserId)..","..KeyTxt.Text)
        elseif fileCheck then
            writefile("PowerStorm_Database.txt", ">"..toBinary(localPlayer.UserId)..","..KeyTxt.Text)
        end
        -- End of Verify
    if KeyTxt.Text == realKey[imptNote] then
        Verification.Text = "Success"
        KeyTxt.TextEditable = false
        loadstring(game:HttpGet"https://raw.githubusercontent.com/Power-Storm/PowerStorm_Hub/main/Games/Hub")()
    else
        Verification.Text = "Invalid Key"
    end
end)

-- Get key Link
KeyBtn.MouseButton1Up:Connect (function()
    setclipboard(realLink[imptNote])
    KeyBtn.Text = "Copied"
    wait(1)
    KeyBtn.Text = "Get Key"
end)

-- Check membership
if table.find(membership,toBinary(localPlayer.UserId)) > 0 then
    KeyTxt.TextEditable = false
    local fileCheck = false
        local userExist = false

        if isfile("PowerStorm_Database.txt") then
            local contents = readfile("PowerStorm_Database.txt")
            local records = contents:split(">")
            if #records > 1 and records[1] == "" then
                for i = 2, #records do
                    local userRecord = records[i]:split(",")
                    if #userRecord == 2 then
                        if userRecord[1] == toBinary(localPlayer.UserId) then
                            userExist = true
                            records[i] = ">"..userRecord[1]..","..realKey[imptNote]--.."\n"
                            else
                            records[i] = ">"..userRecord[1]..","..userRecord[2]--.."\n"
                        end
                    else
                        fileCheck = true
                    end
                end
                if fileCheck == false then
                    writefile("PowerStorm_Database.txt", table.concat(records))
                end
            else
                fileCheck = true
            end
        else
            fileCheck = true
        end

        if userExist == false and fileCheck == false then
            appendfile("PowerStorm_Database.txt", "\n>"..toBinary(localPlayer.UserId)..","..realKey[imptNote])
        elseif fileCheck then
            writefile("PowerStorm_Database.txt", ">"..toBinary(localPlayer.UserId)..","..realKey[imptNote])
        end
    loadstring(game:HttpGet"https://raw.githubusercontent.com/Power-Storm/PowerStorm_Hub/main/Games/Hub")()
end
