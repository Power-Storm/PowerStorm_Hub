-- Buttons
local function Btn1(myBool)
    if myBool == true then
        LogInBtn.BackgroundColor3 = Color3.fromRGB(16, 23, 35)
        LogInBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
        LogInBtn.BorderSizePixel = 0
        LogInFrame.Visible = true
    else
        LogInBtn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        LogInBtn.BorderColor3 = Color3.fromRGB(16, 23, 35)
        LogInBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
        LogInBtn.BorderSizePixel = 1
        LogInFrame.Visible = false
    end
end

